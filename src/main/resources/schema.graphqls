schema {
    query: Query
}

type Query {
  todos: [Todo]!
  getTodoById(id:String!): Todo
	users: [User]!
}

type Todo {
  id: String!
  startDate: Date!
  endDate: Date!
  content: String!
  isAssignedForTeam: Boolean!
  assigned: [User]!
}

type User {
  id: String!
  name: String!
  color: String!
}

type Date {
  year: Int!
  month: Int!
  date: Int!
  day: Int!
  time: Time!
}

type Time {
  hour: Int!
  minute: Int!
  isAm: Boolean!
  isFormat24: Boolean!
}
